{
    "name": "cmsmaxinc/filament-error-pages",
    "description": "Users donâ€™t see blank 404 or 503 error pages. Instead, it shows helpful messages or guides them when something goes wrong.",
    "keywords": [
        "laravel",
        "filament-error-pages",
        "error-pages"
    ],
    "homepage": "https://github.com/cmsmaxinc/filament-error-pages",
    "support": {
        "issues": "https://github.com/cmsmaxinc/filament-error-pages/issues",
        "source": "https://github.com/cmsmaxinc/filament-error-pages"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "CMS Max"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cmsmaxinc\\FilamentErrorPages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmsmaxinc\\FilamentErrorPages\\FilamentErrorPagesServiceProvider"
            ],
            "aliases": {
                "FilamentErrorPages": "Cmsmaxinc\\FilamentErrorPages\\Facades\\FilamentErrorPages"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
